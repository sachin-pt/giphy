{
  "name": "giphy.store",
  "version": "0.0.1",
  "sideEffects": false,
  "author": "Sachin Agrawal <a.sachin533@gmail.com> (https://github.com/sachin-pt)",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.0",
    "yarn": "^1.3.0"
  },
  "scripts": {
    "modules": "yarn",
    "prestart": "yarn && node config",
    "config": "NODE_ENV=production node config",
    "prestart:debug": "yarn && node config",
    "lint-staged": "./node_modules/.bin/lint-staged",
    "lint:js": "node ./node_modules/standard --fix ./**/*.js",
    "precommit": "lint-staged",
    "build:prod": "NODE_ENV=production node scripts/build.js",
    "start": "NODE_ENV=development node --harmony scripts/start.js",
    "start:debug": "NODE_ENV=development DEBUG_MODE=true node --harmony scripts/start.js",
    "clean": "rm -rf build && rm -rf node_modules && rm -rf src/config/appConfig.json",
    "build": "npm run clean && npm run modules && npm run config && npm run build:prod"
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard --print-width 100",
      "node ./node_modules/standard --fix",
      "git add"
    ],
    "*.{png,jpg,jpeg}": [
      "node ./scripts/images/optimize-images",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "__PROD__",
      "__SERVER__",
      "__BUILD_TYPE__",
      "__BUILD_ENV__"
    ],
    "ignore": [],
    "parser": "babel-eslint"
  },
  "moduleRoots": [
    "src/client",
    "src/server",
    "src/shared",
    "src",
    "node_modules"
  ],
  "dependencies": {
    "@apollo/client": "^3.2.3",
    "@emotion/babel-preset-css-prop": "^10.0.23",
    "@emotion/core": "^10.0.22",
    "@loadable/component": "^5.10.3",
    "@loadable/server": "^5.10.3",
    "apollo-server-express": "^2.18.2",
    "babel-eslint": "^10.0.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^3.4.4",
    "emotion-theming": "^10.0.27",
    "express": "^4.17.1",
    "express-useragent": "^1.0.13",
    "graphql": "^15.3.0",
    "history": "^4.10.1",
    "intersection-observer": "^0.5.1",
    "jsec": "^1.0.1",
    "json-stringify-safe": "^5.0.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "preact": "^10.3.3",
    "preact-render-to-string": "^5.1.4",
    "query-string": "^6.11.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "source-map-support": "^0.5.16",
    "unused-files-webpack-plugin": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@loadable/babel-plugin": "^5.10.3",
    "@loadable/webpack-plugin": "^5.2.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "file-loader": "^2.0.0",
    "husky": "^1.1.2",
    "install-deps-postmerge": "^1.0.5",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.19.4",
    "parallel-webpack": "^2.4.0",
    "prettier": "^1.19.1",
    "prettier-standard": "^8.0.1",
    "rimraf": "^2.7.1",
    "speed-measure-webpack-plugin": "^1.3.1",
    "standard": "^12.0.1",
    "thread-loader": "^2.1.3",
    "tinify": "^1.6.0-beta.2",
    "url-loader": "^1.1.2",
    "webpack": "4.30.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
